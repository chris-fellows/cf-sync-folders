<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>	 
	 <add key="DataFolder" value="{process-folder}\Configuration"/>
	 <add key="LogsFolder" value="{process-folder}\Logs\{machine}"/>

	  <add key="EventHandler.Email.Server" value="smtp-mail.outlook.com"/>
	  <add key="EventHandler.Email.Port" value="587"/>
	  <add key="EventHandler.Email.Username" value=""/>
	  <add key="EventHandler.Email.Password" value=""/>
	  
	  <!--Debugging
	 <add key="DataFolder" value="Z:\Data\Applications\CFSyncFolders\Configuration"/>
	 <add key="LogsFolder" value="Z:\Data\Applications\CFSyncFolders\Logs\{machine}"/>
	 -->
	 <add key="Debug" value="true"/>
     <add key="AutoSync.FrequencyMins" value="30"/>
	 <!-- Currently only support local file system. Will support cloud later -->
     <add key="Folder1.FileRepositoryClass" value="LocalRepository"/>
     <add key="Folder2.FileRepositoryClass" value="LocalRepository"/>                  
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup></configuration>
